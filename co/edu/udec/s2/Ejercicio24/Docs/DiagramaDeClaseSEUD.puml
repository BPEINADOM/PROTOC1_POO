@startuml Sistema Educativo 

class CentroEducativo{
    - String nombre;
    - String direccion;
    - List<Curso> cursos;
    - List<Profesor> profesores;
}

class Curso{
    - int id;
    - String nombre;
    - Profesor profesor;
    - Aula aula;
}

class Profesor{
    - String nombre;
    - String apellidos;
    - String correo;
    - String especializacion;
    - Date fecha_nacimiento;
}

class Estudiante{
    - String nombre;
    - String apellidos;
    - String correo;
    - Date fecha_nacimiento;
}

class Evaluacion{
    - String nombre;
    - Estudiante estudiante;
    - Profesor profesor;
    - Curso curso;
}

class Aula{
    - String nombre;
    - Curso curso;
}

class Horario{
    - String nombre;
    - String evento;
    - Date fecha;
    - Curso curso;
}

class Material{
    - String nombre;
    - Curso curso;
    - Profesor profesor;
}

